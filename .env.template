# Snowflake Data Cloud (for historical analytics)
VITE_SNOWFLAKE_ACCOUNT_URL=https://<your_account>.<region>.snowflakecomputing.com
VITE_SNOWFLAKE_TOKEN=<oauth_token>

# AWS Configuration - Disable SSO profile to prevent boto3 authentication errors
# The snowflake-connector-python imports boto3 which reads ~/.aws/config by default
# Setting AWS_PROFILE to empty prevents it from using SSO profiles
AWS_PROFILE=
AWS_SDK_LOAD_CONFIG=false

# Snowflake Managed Postgres (for real-time operations)
# Create a Snowflake Postgres instance and update these values
VITE_POSTGRES_HOST=<your_postgres_instance>.<account>.postgres.snowflake.app
VITE_POSTGRES_PORT=5432
VITE_POSTGRES_DATABASE=postgres
VITE_POSTGRES_USER=<your_username>
VITE_POSTGRES_PASSWORD=<your_password>

# Snowflake Connection (for CLI tools)
SNOWFLAKE_CONNECTION_NAME=<your_connection_name>
